<div class="breadcrumb">
	<% columns.each_with_index do |col,index| %>
		<% col,type=col.split(':') %>
	  <div class="row">
	      <strong><%= tt(captions[index]) %></strong>:
        <% if @obj.is_a?(Hash) %>
          <%= @obj[col] %>
        <% else %>
          <%=  get_attr(obj,col) %>
        <% end %>
	  </div>
	<% end %>
</div>
<%#= debug columns %>
<%#= debug captions %>
